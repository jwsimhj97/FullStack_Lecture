package com.rom802;

public class Ex02 {

	public static void main(String[] args) {
		// º¯¼ö
		byte a;
		short b;
		int c;
		long d;
		a=1;
		b=2;
		c=3;
		d=4L;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		
		float e;
		double f;
		e=3.14f;
		f=3.14;
		
		System.out.println(e);
		System.out.println(f);
		
		char g;
//		g='A';
		g='ÇÑ';
		
		System.out.println(g);
		
		boolean boo;
		
		boo=true;
		System.out.println(boo);
		
		d=c
	}
}
