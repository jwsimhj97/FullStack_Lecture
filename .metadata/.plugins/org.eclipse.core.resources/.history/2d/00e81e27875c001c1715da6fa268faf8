package rom802;

public class Ex05 {

	public String msg;
	
	public Ex05(){
		msg="hello";
		for(int i=0; i<1; i++){
			System.out.println(msg);
		}
	}
	
	public Ex05(String msg){
		for(int i=0; i<1; i++){
			System.out.println(msg);
		}
	}
	

	public Ex05(String msg, int cnt){
		for(int i=0; i<cnt; i++){
			System.out.println(msg);
		}
	}
	
	public static void main(String[] args) {
//		Ex05 me=new Ex05();
		
		Ex05 me=new Ex05("java");
	}

}
