package com.rom802;

public class Ex01 {

	public static void main(String[] args) {
		for (int i=0; i<3; i++) {
			for (int j=0; j<3; j++) {
				System.out.print(ㅓ);
			}
			System.out.println();
		}
	}

}

// 3 줄을 찍는데 그 한 줄 한줄에서도 별이 세 번 반복이다
