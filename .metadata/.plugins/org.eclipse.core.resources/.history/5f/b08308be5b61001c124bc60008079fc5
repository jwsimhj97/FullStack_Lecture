package com.rom802;

class MyException{
	
}

public class Ex08 {

	static void func(int a, int b) throws MyException{
		if(b==1){
			MyException ex=new MyException();
			throw ex;
		}
		int c=a/b;
		System.out.println(a+"/"+b+"="+c);
	}
	
	public static void main(String[] args) {
		// 근데 만약 우리한테는 1로 나누는 상황이 에러라면 그 때의 Exception 클래스가 필요
		try{
			func(6,2);			
		}catch(MyException e){
			
		}
	}

}
