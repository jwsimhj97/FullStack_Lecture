package com.hi;

public class Ex04 {
	
	public static void func01() {
		System.out.println("새롭게 만든 func01");
	}

	public static void main(String[] args) {		// main 또한 메소드임
		// 메서드
		// public static void 메서드명([매개변수들...]) { 구현코드; }
		// 메서드명 - 영문 소문자로 시작
		// 호출을 받아야지만 실행 (해당 메서드로 가서 일을 하고 돌아온다)
		// 메서드명(); // 으로 호출
		
		System.out.println("기존 main");
		func01();
		func02(1234);
	}
	
	
	public static void func02(int su) {
		System.out.println("새롭게 만든 func02");
	}
	
}
