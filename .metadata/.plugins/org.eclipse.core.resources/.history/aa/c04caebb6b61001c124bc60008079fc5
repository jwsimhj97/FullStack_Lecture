package com.rom802;

class Ball{
	private int num;	// 공 번호 바뀌면 안되니까
	boolean marking;
	Ball(int num){
		this.num=num;
		marking=false;
	}
	
}

public class Ex12 {

	public static void main(String[] args) {
		Ball[] box=new Ball[45];
		for(int i=0; box.length; i++){
			box[i]=new Ball(i+1);
		}

	}

}
