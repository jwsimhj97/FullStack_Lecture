package com.rom802;

public class Ex03 {

	public static void main(String[] args) {
		
		//q1
		System.out.println("Q1. 1~10까지 짝수들을 출력하시오");
		for (int i=1; i<=10; i++) {
			if (i%2 == 0) System.out.println(i);
		}
		
		//q2
		System.out.println("\nQ2. A~Z까지 출력하시오");
		
		//q3
		System.out.println("Q3. 1, 3, 5, 7, 9 를 출력하시오");
		
		//q4
		System.out.println("Q4. 1+3+5+7+9=000를 출력하시오");
		
		//q5
		System.out.println("Q5. 1~50 중 4의 배수를 출력하시오");
	}
}
