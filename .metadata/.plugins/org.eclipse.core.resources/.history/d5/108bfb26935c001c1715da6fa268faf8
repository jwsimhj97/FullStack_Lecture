package rom802;

public class Ex07 {

	public static int inputNum(){
		String input;
		int num;
		
		java.util.Scanner sc=new java.util.Scanner(System.in);
		input=sc.nextLine();
		num=Integer.parseInt(input);
		return num;
	}
	
