package com.rom802;

public class Ex05 {

	public static void main(String[] args) {
		
		
		for (int i=0; i<3; i++) {
			for (int j=0; j<3; j++) {	// 이런 2중 for문에서 무엇을 출력할 지 선택하는 것임
				if (j<2-i) {
					System.out.print(' ');
				} else {
					System.out.print('*');					
				}
			}
			System.out.println();
		}

	}

}
