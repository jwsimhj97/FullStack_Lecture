package com.rom802;

public class Ex10 {

	public static void main(String[] args) {
		System.out.println(Math.abs(12));
		System.out.println(Math.abs(-12));
		double su=3.54;
		System.out.println(Math.ceil(su));	//올림
		System.out.println(Math.floor(su));	//버림
		System.out.println(Math.round(su));	//반올림
		System.out.println(su);
	}
	
}
