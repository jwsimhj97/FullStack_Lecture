package com.rom802;

public class Ex01 {

	public static void main(String[] args) {
		System.out.println("로또 번호 생성기(ver 0.0.1)\n");
		
		final int ROTTO=10;
		int		result[]=new int[ROTTO];
		int		num = 0;
		int		cnt = 0;
		
		
		for(int i=0; i<ROTTO; i++){
			num = (int)(Math.random()*45)+1;
			int j=0;
			while(j<cnt){
				if(num==result[j]){
					j=0;
					continue;
				}
				j++;
			}
			System.out.println(num);
			result[i]=num;
			cnt++;
		}
		for(int i=0; i<ROTTO; i++){
			System.out.print(result[i]+" ");			
		}
	}

}
