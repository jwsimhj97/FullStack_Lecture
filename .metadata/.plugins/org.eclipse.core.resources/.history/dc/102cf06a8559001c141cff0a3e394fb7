package com.rom802;

public class Ex06 {

	public static void main(String[] args) {
		// 배열의 복사
		int[] arr1=new int[]{1,3,5,7,9};
		int[] arr2=new int[]{1,3,5,7,9};
		System.out.println(arr1==arr2);
		int[] arr3=arr1;
		System.out.println(arr1==arr3);
		int[] arr4=new int[arr1.length];
		for(int i=0; i<arr1.length; i++) {
			System.out.println();
		}
	}

}
