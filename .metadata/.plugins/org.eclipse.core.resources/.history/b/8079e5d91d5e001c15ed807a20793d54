package rom802;

class Tv{
	public void on(){
		System.out.println("켜다");
	}
	public void off(){
		System.out.println("끄다");
	}
	public void work(){
		System.out.println("영상을 보여준다");
	}
}


class Radio{
	public void on(){
		System.out.println("켜다");
	}
	public void off(){
		System.out.println("끄다");
	}
	public void word(){
		System.out.println("주파수를 잡아서 소리를 들려준다");
	}
}


