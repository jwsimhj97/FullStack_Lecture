package day211209;

public class Ex05 {

	public static int c;
	public static int d;
	
	public static void func01(int a, int b) {
		System.out.println(a+b);
	}
	
	public static void func02() {
		System.out.println();
	}
	
	public static void main(String[] args) {
		int a,b;
		
		a=2;
		b=3;
		func01(a, b);
	}

}
