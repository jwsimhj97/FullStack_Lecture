package com.rom802;

class Ex33 {
	int a=1111;
}

class Ex333 {
	int b=2222;
	
	public Ex333() {
		System.out.println("생성자 호출");
	}
}

class Ex3333 {
	int c=3333;
	public Ex3333(int su) {
		c=su;
		System.out.println(c+"를 매개로 받는 생성자");
	}
}

public class Ex03 {
	public static void main(String[] args) {
		Ex33 ex33 = new Ex33();
		Ex333 ex333 = new Ex333();
	}
}
