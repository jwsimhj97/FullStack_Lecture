package com.rom802;

public class Ex13 {
	int su=1234;
	
	public static void main(String[] args) {
		// 자바 리플렉션
		Ex13 me=new Ex13();
		System.out.println(me.su);
		// 리플렉션을 이용해서 객체를 찍어볼게요. 생성자 말고
		
		// 클래스에 대한 정보 필요
		String info="com.rom802.Ex13";		// 근데 이건 그냥 문자열인데 이걸 이용해서
		Class cls=Class.forName(info)
	}

}
