package rom802;

class Lec02{
	public int su1=1111;
	
	public void func01(){
		System.out.println("부모");
	}
}

public class Ex02 extends Lec02{


	public int su1=2222;		// 부모 것을 상속 받아서 필요가 없는데 만약 나한테도 있다면??
	
	public void func01(){
		System.out.println("자식");
	}
	
	public static void main(String[] args) {
		Ex02 me=new Ex02();
		System.out.println(me.su1);
		me.func01();
	}

}
