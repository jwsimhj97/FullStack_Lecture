package com.rom802;

public class Ex01 {

	public static void main(String[] args) {
		System.out.println("로또 번호 생성기(ver 0.0.1)\n");
		
		int		result[]=new int[6];
		int		num = 0;
		int		cnt = 0;
		boolean boo=true;
		
		
		for(int i=0; i<6; i++){
			num = (int)(Math.random()*45)+1;
			for(int j=0; j<cnt; j++){
				if(num==result[j]){
					boo=false;
				}
			}
			result[i]=num;
			cnt++;
		}
		for(int i=0; i<6; i++){
			System.out.print(result[i]+" ");			
		}
	}

}
