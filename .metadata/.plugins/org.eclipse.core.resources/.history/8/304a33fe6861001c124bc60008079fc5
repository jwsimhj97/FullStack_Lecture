package com.rom802;

public class Ex11 {

	public static void main(String[] args) {
		int[] lotto=new int[45];
		int ran;
		
		for(int i=0; i<lotto.length; i++){
			lotto[i]=i+1;
		}
		
		for(int i=0; i<10000; i++){
			lotto[0]=lotto[ran];
		}
		
		for(int i=0; i<6; i++){
			System.out.println(lotto[i]+" ");
		}
	}

}
