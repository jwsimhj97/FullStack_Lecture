package com.rom802;

public class Ex11 {

	public static void main(String[] args) {
		int[] lotto=new int[45];
		int ran;
		int temp;
		
		for(int i=0; i<lotto.length; i++){
			lotto[i]=i+1;
		}
		
		for(int i=0; i<10000; i++){
			ran=(int)(Math.random()*45);
			temp=lotto[0];			// ÀÌ 0ÀÌ¶ó´Â ¼ýÀÚ´Â »ó°üÀÌ ¾øÀ½
			lotto[0]=lotto[ran];	// ·£´ýÇÑ ÀÎµ¦½º¸¦ »Ì¾Æ¼­ ¹Ù²ãÁÜ
			lotto[ran]=temp;
		}
		
		for(int i=0; i<6; i++){
			System.out.print(lotto[i]+" ");
		}
	}

}
