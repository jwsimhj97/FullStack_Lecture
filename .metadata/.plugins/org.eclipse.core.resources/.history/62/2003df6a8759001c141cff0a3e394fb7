package com.rom802;

public class Ex07 {

	public static void func01(int[] arr2) {
		arr2[1] = 10;
	}
	
	public static void main(String[] args) {
		// 얕은 복사
		int[] arr1 = {2,4,6,8};
		
		func01(arr1);	// 이 함수에서 배열을 전달했는데 옅은 복사가 이루어지고 있다 
		
		for (int i=0; i<arr1.length; i++) {
			System.out.println(arr1[i]);
		}

	}

}
