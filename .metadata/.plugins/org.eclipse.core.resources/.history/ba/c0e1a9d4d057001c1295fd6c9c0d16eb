package com.hi;

public class Ex04 {
	
	public static void func01() {
		System.out.println("새롭게 만든 func01");
	}

	public static void main(String[] args) {
		// 메서드
		System.out.println("기존 main");
		func01();
		func02();
	}
	
	
	public static void func02() {
		System.out.println("새롭게 만든 func02");
	}
}
