package com.rom802;

// '중복 검사'와 '정렬'을 같은 반복문에서 처리

public class Ex01_1_1_2 {

	public static void main(String[] args) {
		
		int[] lotto=new int[6];
		

		lotto[0]=(int)(Math.random()*6)+1;
		
		boolean dup = false;
		int i=1;
		while(i<6){
			for(int j=i; j<6; j++){
				lotto[i]=(int)(Math.random()*6)+1;
				if (lotto[i]==lotto[i-1]){
					dpu=true;
					continue();
				}
			}
			i++;
		}

	}

}
